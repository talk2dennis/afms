{
  "expo": {
    "name": "afms",
    "slug": "afms",
    "scheme": "afms",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/afms_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/afms_splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#abdbe3"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show nearby local governments."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/afms_logo.png",
        "backgroundColor": "#abdbe3"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.talk2dennis.afms",
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    },
    "web": {
      "favicon": "./assets/afms_logo.png"
    },
    "plugins": ["expo-router", "expo-secure-store", "expo-font"],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3807dfd9-3a78-4768-a843-3ed35097d5ad"
      }
    }
  }
}
